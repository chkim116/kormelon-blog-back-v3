{
  "name": "kormelon-blog-back-v3",
  "version": "1.1.8",
  "description": "kormelon blog server",
  "main": "./dist/app.js",
  "scripts": {
    "test": "jest",
    "env": "node_modules/cross-env/src/bin/cross-env.js",
    "build": "swc src -d dist",
    "build:dev": "env NODE_ENV=development env-cmd -f .env.development npm run build",
    "build:prod": "env NODE_ENV=production npm run build",
    "generate-eb-zip": "sh ./scripts/eb-zip.sh",
    "dev": "nodemon --ext ts --exec \"npm run build:dev && env NODE_ENV=development env-cmd -f .env.development node --experimental-specifier-resolution=node --inspect dist/app.js\"",
    "preview": "npm run build:prod && env NODE_ENV=production env-cmd -f .env.production node --experimental-specifier-resolution=node dist/app.js",
    "start": "env NODE_ENV=production node --experimental-specifier-resolution=node dist/app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chkim116/kormelon-blog-back-v3.git"
  },
  "keywords": [
    "blog",
    "blog server"
  ],
  "author": "kimchanghoe <cskim132@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chkim116/kormelon-blog-back-v3/issues"
  },
  "homepage": "https://github.com/chkim116/kormelon-blog-back-v3#readme",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.171.0",
    "aws-sdk": "^2.1218.0",
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-rate-limit": "^6.7.0",
    "express-validator": "^6.14.2",
    "helmet": "^6.0.0",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "mysql2": "^2.3.3",
    "node-schedule": "^2.1.0",
    "reading-time": "^1.5.0",
    "request-ip": "^3.3.0",
    "typeorm": "^0.2.45"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.245",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.8",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/multer-s3": "^3.0.0",
    "@types/node": "^18.7.13",
    "@types/node-schedule": "^2.1.0",
    "@types/request-ip": "^0.0.37",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "cross-env": "^7.0.3",
    "env-cmd": "^10.1.0",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.0.1",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "reflect-metadata": "^0.1.13",
    "typescript": "^4.8.2"
  },
  "type": "module"
}
