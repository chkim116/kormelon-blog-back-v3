{
  "name": "kormelon-blog-back-v3",
  "version": "0.0.2",
  "description": "kormelon blog server",
  "main": "./dist/app.js",
  "scripts": {
    "test": "jest",
    "build": "swc src -d dist",
    "build:dev": "env-cmd -f .env.development npm run build",
    "build:prod": "env-cmd -f .env.production npm run build",
    "dev": "nodemon --ext ts --exec \"npm run build:dev && cross-env NODE_ENV=development env-cmd -f .env.development node --experimental-specifier-resolution=node --inspect dist/app.js\"",
    "start": "npm run build:prod && cross-env NODE_ENV=production env-cmd -f .env.production node --experimental-specifier-resolution=node dist/app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chkim116/kormelon-blog-back-v3.git"
  },
  "keywords": [
    "blog",
    "blog server"
  ],
  "author": "kimchanghoe <cskim132@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chkim116/kormelon-blog-back-v3/issues"
  },
  "homepage": "https://github.com/chkim116/kormelon-blog-back-v3#readme",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "helmet": "^6.0.0",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.3.3",
    "typeorm": "^0.3.9"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.245",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.8",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.7.13",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "cross-env": "^7.0.3",
    "env-cmd": "^10.1.0",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.0.1",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "reflect-metadata": "^0.1.13",
    "typescript": "^4.8.2"
  },
  "type": "module"
}
